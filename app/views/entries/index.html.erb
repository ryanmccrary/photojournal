<div class="page-header">
<h4>Photojournal Entries</h4>
</div>

<%= current_user.id %>

<% @entries.each do |entry| %>

<div class="row">
<div class="entry">
  <div class="span4">
    <%= image_tag entry.photo %>
  </div>
  <div class="span8">
    <strong><%= entry.copy %></strong>
    <div class="meta">
    <%= link_to time_ago_in_words(entry.created_at) + " ago", entry %>
    </div>
  </div>
</div>
</div>

<% end %>
