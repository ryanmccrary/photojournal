<p id="notice"><%= notice %></p>

<p>
  <b>Photo:</b>
  <%= @entry.photo %>
</p>
<%= image_tag @entry.image_url(:thumb).to_s %>
<p>
  <b>Copy:</b>
  <%= @entry.copy %>
</p>


<%= link_to 'Edit', edit_entry_path(@entry) %> |
<%= link_to 'Back', entries_path %> | <%= link_to "Delete", @entry, method: :delete, data: { confirm: "Are you sure you want to delete this entry? Forever? Forever-ever?"} %>
